{
  "name": "pst-extractor",
  "version": "1.3.3",
  "description": "Extract objects from MS Outlook/Exchange PST files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "nyc --reporter=html mocha --opts mocha.opts"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/index.ts",
      "src/app/Log.class.ts",
      "coverage/prettify.js",
      "coverage/sorter.js"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epfromer/pst-extractor.git"
  },
  "keywords": [],
  "author": "Ed Pfromer (epfromer@gmail.com)",
  "license": "(Apache-2.0 OR GPL-3.0)",
  "bugs": {
    "url": "https://github.com/epfromer/pst-extractor/issues"
  },
  "homepage": "https://github.com/epfromer/pst-extractor#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/debug": "4.1.1",
    "@types/long": "^4.0.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.3",
    "chai": "^4.2.0",
    "chai-datetime": "^1.5.0",
    "debug": "^4.1.1",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.10",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "long": "^4.0.0",
    "uuid-parse": "^1.0.0"
  }
}
